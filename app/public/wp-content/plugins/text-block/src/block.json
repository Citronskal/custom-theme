{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/text-block",
	"version": "0.1.0",
	"title": "Text Block",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "text-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}

{
    "apiVersion": 2,
    "name": "my-plugin/two-column-block",
    "title": "Two Column Block",
    "category": "common",
    "attributes": {
        "text": {
            "type": "string",
            "source": "html",
            "selector": "p"
        },
        "mediaURL": {
            "type": "string",
            "source": "attribute",
            "selector": "img",
            "attribute": "src"
        }
    },
	"style": "file:./style.css",
    "editorStyle": "file:./editor.css",
    "edit": "./edit.js",
    "save": "./save.js"
}